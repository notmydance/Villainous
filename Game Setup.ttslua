function onLoad()
  isOpen = false
  setupButt = "Open Game Setup"
  gamePickButtonGo()
  self.interactable = false
end

function gamePickButtonGo()
    isOpen = false
    self.createButton({click_function="drawUI", function_owner=self,
    position={0,0,0}, rotation={90,0,90}, width=350, height=350,
    tooltip=setupButt})
    self.createButton({click_function="drawUI", function_owner=self,
    position={0,0,0}, rotation={0,90,0}, width=345, height=345,
    tooltip=setupButt})
end

function drawUI()
    if (isOpen ~= true) then
    setupButt = "Close Game Setup"
    self.clearButtons()
    gamePickButtonGo()
    local tooltip1="Spawn Disney Villainous"
    local tooltip2="Spawn Marvel Villainous"
    local tooltip3="Spawn Star Wars Villainous"
    local tooltip4="Spawn Custom Villains"
    local tooltip5="Toggle Extra Options"
    self.createButton({click_function="spawnDis", function_owner=self,
        position={0,0,-2.5}, rotation={0,180,0}, width=1500, height=440,
        color={0,.75,0}, font_size=150, tooltip=tooltip1, label="Disney Villainous"})
    self.createButton({click_function="spawnMar", function_owner=self,
        position={0,0,-3.5}, rotation={0,180,0}, width=1500, height=440,
        color={1,1,0}, font_size=150, tooltip=tooltip2, label="Marvel Villainous"})
    self.createButton({click_function="spawnStar", function_owner=self,
        position={0,0,-4.5}, rotation={0,180,0}, width=1500, height=440,
        color={0.15,0.25,0.9}, font_size=150, tooltip=tooltip3, label="Star Wars Villainous"})
    self.createButton({click_function="listCus", function_owner=self,
        position={0,0,-5.5}, rotation={0,180,0}, width=1500, height=440,
        color={0.625,0.125,0.9375}, font_size=150, tooltip=tooltip4, label="     Custom Villains  ▶"})
    self.createButton({click_function="listOpt", function_owner=self,
        position={0,0,-6.5}, rotation={0,180,0}, width=1500, height=440,
        color={1,1,1}, font_size=150, tooltip=tooltip5, label="◀   Extra Options      "})
    isOpen = true
    else
    self.clearButtons()
    setupButt = "Open Game Setup"
    gamePickButtonGo()
    end
end

