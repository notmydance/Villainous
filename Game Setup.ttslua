function clearTable()
    isCus = false
    isList = false
    resetHands()
    local clear = getObjectFromGUID('5b9b72').getObjects()
    for k,v in pairs(clear) do
        v.destruct()
    end
    self.clearButtons()
    isOpen = false
    gamePickButtonGo()
    setNotes("")
end

